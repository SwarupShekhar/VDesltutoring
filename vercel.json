{
    "headers": [
        {
            "source": "/:path((?:sitemap.xml|robots.txt))",
            "headers": [
                {
                    "key": "X-Robots-Tag",
                    "value": "all"
                }
            ]
        },
        {
            "source": "/:path*",
            "headers": [
                {
                    "key": "X-Robots-Tag",
                    "value": "all"
                }
            ]
        }
    ],
    "redirects": [
        {
            "source": "/:path*",
            "has": [
                {
                    "type": "host",
                    "value": "www.englivo.com"
                }
            ],
            "destination": "https://englivo.com/:path*",
            "statusCode": 301
        },
        {
            "source": "/:path*",
            "has": [
                {
                    "type": "header",
                    "key": "x-forwarded-proto",
                    "value": "http"
                }
            ],
            "destination": "https://englivo.com/:path*",
            "statusCode": 301
        }
    ]
}